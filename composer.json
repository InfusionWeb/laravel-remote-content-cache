    "keywords": [
        "laravel",
        "remote",
        "content",
        "cache",
        "headless cms"
    ],
    "homepage": "https://github.com/infusionweb/laravel-remote-content-cache",
    "license": "MIT",
    "authors": [
        {
            "name": "Russell Keppner",
            "email": "russell.keppner@gmail.com",
            "homepage": "https://github.com/rkeppner",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "ext-gd": "*",
        "kozz/laravel-guzzle-provider": "^7.1",
        "intervention/image": "~2.3"
    },
    "require-dev": {
        "codeclimate/php-test-reporter": "dev-master",
        "phpunit/phpunit": "~4.6"
    },
    "autoload": {
        "psr-4": {
            "InfusionWeb\\Laravel\\": "src/"
        }
